    "@angular/platform-browser-dynamic": "^4.3.3",
    "@angular/router": "^4.3.3",
    "angular2-moment": "1.7.0",
    "angularfire2": "4.0.0-rc.2",
    "angularfire2-offline": "4.3.1",
    "core-js": "^2.4.1",
    "firebase": "^4.3.1",
